@model EducacionVirtual.Models.evaluaciones.Actividades

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Actividades</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TipoEvaluacion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoEvaluacion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreActividad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreActividad)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Docente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Docente.NombreCompletoD)
        </dd>
    </dl>
</div>
<div>
    <button type="button" class="btn btn-warning">
        <a style="text-decoration:none" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
    </button>
    <button type="button" class="btn btn-dark">
        <a style="text-decoration:none" asp-action="Index">Regresar a la lista</a>
    </button>
    
</div>
